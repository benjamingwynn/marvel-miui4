echo "Patching services.jar"
cd /Volumes/Cooking/Temporary-old
baksmali -o services services.jar
cd /Volumes/Cooking/Temporary-new
baksmali -o services services.jar
cd /Volumes/Cooking
cp -v Temporary-old/services/com/android/server/WiredAccessoryObserver*.smali Temporary-new/services/com/android/server
cp -v Temporary-old/services/com/android/server/usb Temporary-new/services/com/android/server
cp -v Temporary-old/services/com/android/server/location Temporary-new/services/com/android/server
cp -v Temporary-old/services/com/android/server/LocationManagerService*.smali Temporary-new/services/com/android/server
echo "manual patch following files"
echo "Add methods nativesetkeylayout and nativesetstylusiconenabled to inputmanager.smali"
scipts/press_enter
echo "Add methods nativesurfaceflingeroffanimation/onanimation to powermanagerservice.smali"
scipts/press_enter
cd /Volumes/Cooking/Temporary-new
smali -o classes.dex services
zip -m services.jar classes.dex
rm classes.dex
cd /Volumes/Cooking
cp -v Temporary-new/services.jar WORKING_MIUI/system/framework
echo "Patching Done"